{
  "dashboard": {
    "title": "RAG Benchmark Dashboard",
    "uid": "rag_benchmark",
    "tags": ["rag", "benchmark", "petroleum"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "10s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Query Rate (queries/sec)",
        "type": "graph",
        "gridPos": {
          "x": 0,
          "y": 0,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "rate(rag_queries_total[5m])",
            "legendFormat": "{{parser}} - {{storage}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "label": "queries/sec",
            "format": "short"
          },
          {
            "label": null,
            "format": "short"
          }
        ]
      },
      {
        "id": 2,
        "title": "Query Duration (95th percentile)",
        "type": "graph",
        "gridPos": {
          "x": 12,
          "y": 0,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(rag_query_duration_seconds_bucket[5m]))",
            "legendFormat": "{{parser}} - {{storage}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "label": "seconds",
            "format": "s"
          },
          {
            "label": null,
            "format": "short"
          }
        ]
      },
      {
        "id": 3,
        "title": "Embedding Generation Rate",
        "type": "graph",
        "gridPos": {
          "x": 0,
          "y": 8,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "rate(rag_embeddings_total[5m])",
            "legendFormat": "{{status}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "label": "embeddings/sec",
            "format": "short"
          },
          {
            "label": null,
            "format": "short"
          }
        ]
      },
      {
        "id": 4,
        "title": "Embedding Duration (by batch size)",
        "type": "graph",
        "gridPos": {
          "x": 12,
          "y": 8,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(rag_embedding_duration_seconds_bucket[5m]))",
            "legendFormat": "{{batch_size_range}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "label": "seconds",
            "format": "s"
          },
          {
            "label": null,
            "format": "short"
          }
        ]
      },
      {
        "id": 5,
        "title": "LLM Call Rate",
        "type": "graph",
        "gridPos": {
          "x": 0,
          "y": 16,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "rate(rag_llm_calls_total[5m])",
            "legendFormat": "{{model}} - {{status}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "label": "calls/sec",
            "format": "short"
          },
          {
            "label": null,
            "format": "short"
          }
        ]
      },
      {
        "id": 6,
        "title": "LLM Call Duration (95th percentile)",
        "type": "graph",
        "gridPos": {
          "x": 12,
          "y": 16,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(rag_llm_duration_seconds_bucket[5m]))",
            "legendFormat": "{{model}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "label": "seconds",
            "format": "s"
          },
          {
            "label": null,
            "format": "short"
          }
        ]
      },
      {
        "id": 7,
        "title": "Parse Duration by Parser",
        "type": "graph",
        "gridPos": {
          "x": 0,
          "y": 24,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(rag_parse_duration_seconds_bucket[5m]))",
            "legendFormat": "{{parser}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "label": "seconds",
            "format": "s"
          },
          {
            "label": null,
            "format": "short"
          }
        ]
      },
      {
        "id": 8,
        "title": "Cache Hit Rate",
        "type": "graph",
        "gridPos": {
          "x": 12,
          "y": 24,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "rag_cache_hit_rate",
            "legendFormat": "{{cache_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "label": "hit rate",
            "format": "percentunit",
            "min": 0,
            "max": 1
          },
          {
            "label": null,
            "format": "short"
          }
        ]
      },
      {
        "id": 9,
        "title": "Active Connections",
        "type": "graph",
        "gridPos": {
          "x": 0,
          "y": 32,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "rag_active_connections",
            "legendFormat": "{{backend}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "label": "connections",
            "format": "short"
          },
          {
            "label": null,
            "format": "short"
          }
        ]
      },
      {
        "id": 10,
        "title": "Cache Size",
        "type": "graph",
        "gridPos": {
          "x": 12,
          "y": 32,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "rag_cache_size_items",
            "legendFormat": "{{cache_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "label": "items",
            "format": "short"
          },
          {
            "label": null,
            "format": "short"
          }
        ]
      },
      {
        "id": 11,
        "title": "Query Success Rate",
        "type": "stat",
        "gridPos": {
          "x": 0,
          "y": 40,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "sum(rate(rag_queries_total{status=\"success\"}[5m])) / sum(rate(rag_queries_total[5m]))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0,
            "max": 1
          }
        }
      },
      {
        "id": 12,
        "title": "Embedding Success Rate",
        "type": "stat",
        "gridPos": {
          "x": 6,
          "y": 40,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "sum(rate(rag_embeddings_total{status=\"success\"}[5m])) / sum(rate(rag_embeddings_total[5m]))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0,
            "max": 1
          }
        }
      },
      {
        "id": 13,
        "title": "LLM Success Rate",
        "type": "stat",
        "gridPos": {
          "x": 12,
          "y": 40,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "sum(rate(rag_llm_calls_total{status=\"success\"}[5m])) / sum(rate(rag_llm_calls_total[5m]))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0,
            "max": 1
          }
        }
      },
      {
        "id": 14,
        "title": "Documents in Storage",
        "type": "stat",
        "gridPos": {
          "x": 18,
          "y": 40,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "rag_documents_in_storage",
            "legendFormat": "{{storage}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      },
      {
        "id": 15,
        "title": "Storage Operation Duration",
        "type": "graph",
        "gridPos": {
          "x": 0,
          "y": 44,
          "w": 24,
          "h": 8
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(rag_storage_operation_duration_seconds_bucket[5m]))",
            "legendFormat": "{{storage}} - {{operation}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "label": "seconds",
            "format": "s"
          },
          {
            "label": null,
            "format": "short"
          }
        ]
      }
    ]
  }
}
